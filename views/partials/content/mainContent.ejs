    <div class="parallax">
        <div id = "parallax-overlay">
            <p id = "parallax-text">Cryptocurrency & Blockchain</p>
        </div>
    </div>

      <% data.forEach((item)=>{%>
        <h3><%= item.name%> (<%= item.abbreviation %>) <a href="#"><img src="<%= item.logo %>"></a></h3>
        
        <a href="https://www.reddit.com/r/<%=item.name%>">Reddit</a>
        <a href="https://coinmarketcap.com/currencies/<%= item.name%>">Coinmarket Cap</a>
        <a href="<%= item.website%>">Website</a>
        <br>
        
        <h4>Summary</h4>
        <p><%= item.summary%></p>
        <h4>Cons</h4>
        <p id = "cons"><%= item.cons%></p>
    <% })%>